ate } from 'react'\r\nimport { dictionary } from '../dictionary/data'\r\nimport firebase, { db } from '../../services/firebase'\r\nimport { storage } from '../../services/firebase'\r\n\r\nexport const CreateWord = () => {\r\n\r\n    const [word, setWord] = useState('');\r\n    const [pinin, setPinin] = useState('');\r\n    const [definition, setDefinition] = useState('');\r\n    const [audioUrl, setAudioUrl] = useState('');\r\n\r\n    const clearHandler = () => {\r\n        setWord('');\r\n        setPinin('');\r\n        setDefinition('');\r\n    }\r\n\r\n    // подключение к БД \r\n    const uploadDictionary = async (dictionary) => {\r\n        const preparedWord = {\r\n            word: { word },\r\n            pinin: { pinin },\r\n            definition: { definition },\r\n            audioUrl: { audioUrl }\r\n        }\r\n        const newDoc = await db.collection('Dictionary').add(preparedWord);\r\n    }\r\n\r\n    const uploadFile = async (evt) => {\r\n        const { files } = evt.target;\r\n        const snapshotRef = storage.child(`/audio/${files[0].name}`)\r\n        const snapshot = await snapshotRef.put(files[0])\r\n        const fileUrl = await snapshot.ref.getDownloadURL();\r\n        // токен нужно откорректировать\r\n        const {origin, pathname} = new URL(fileUrl);\r\n        const preparedAudioUrl = `${origin}${pathname}?alt=media`\r\n        setAudioUrl(preparedAudioUrl)\r\n    }\r\n\r\n    return (\r\n        <div className=''>\r\n            <form className='word__creator'>\r\n                <label className='word__label'>Слово\r\n                    <input\r\n                        type='text'\r\n                        value={word}\r\n                        onChange={(evt) => setWord(evt.target.value)}\r\n                    /></label>\r\n                <label className='word__label'>Пиньинь\r\n                    <input\r\n                        type='text'\r\n                        value={pinin}\r\n                        onChange={(evt) => setPinin(evt.target.value)}\r\n                    /></label>\r\n                <label className='word__label'>Определение\r\n                    <input\r\n                        type='text'\r\n                        value={definition}\r\n                        onChange={(evt) => setDefinition(evt.target.value)}\r\n                    /></label>\r\n                <label>\r\n                    <input\r\n                        type='file'\r\n                        onChange={uploadFile}\r\n                    />\r\n                </label>\r\n            </form>\r\n            <button\r\n                type='button'\r\n                onClick={() => uploadDictionary(dictionary)}\r\n            >UploadDictionary\r\n                </button>\r\n            <button\r\n                type='button'\r\n                onClick={clearHandler}\r\n            >Clear</button>\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\n","C:\\Users\\Ultra\\Desktop\\Practice\\wisdom\\src\\services\\dictionary.js",["127"],"import { db } from \"./firebase\";\r\nimport { dictionary } from '../components/dictionary/data'\r\n\r\nexport const getDictionary = async () => {\r\n    const dictionaryCollection = db.collection(\"Dictionary\");\r\n\r\n    try {\r\n        const allWordsSnapShot = await dictionaryCollection.get();\r\n\r\n        let allWords = [];\r\n\r\n        // prepare all words for client\r\n        allWordsSnapShot.forEach((doc) => {\r\n            const unpackedWord = doc.data()\r\n            allWords.push({\r\n                key: doc.id,\r\n                definition: unpackedWord.definition.definition,\r\n                pinin: unpackedWord.pinin.pinin,\r\n                word: unpackedWord.word.word,\r\n                audioUrl: unpackedWord?.audioUrl?.audioUrl,\r\n                //   ^ optional chaining ^\r\n            })\r\n        });\r\n\r\n        const preparedAllWords = allWords.map((word, i) => {\r\n            return {\r\n                ...word,\r\n                id: i + 1\r\n            }\r\n        })\r\n        return preparedAllWords\r\n    } catch (error) {\r\n        return Promise.reject(error);\r\n    }\r\n}\r\n\r\n\r\n","C:\\Users\\Ultra\\Desktop\\Practice\\wisdom\\src\\components\\practice\\practice-menu.js",["128","129","130"],"import { React } from 'react'\r\nimport { Route, BrowserRouter as Router, Link, Switch } from 'react-router-dom'\r\n\r\nexport const PracticeMenu = () => {\r\n    // contest sub-menu\r\n    // перенос switch в отдельный\r\n    return (\r\n        <div className='container'>\r\n            <nav className='practice-navigation'>\r\n                <ul className='practice-navigation__menu'>\r\n                    <li className='practice-navigation__menu__section'><Link to='/type-contest'>Письменный спринт</Link></li>\r\n                    <li className='practice-navigation__menu__section'><Link to='/audio-contest'>Аудирование</Link></li>\r\n                    <li className='practice-navigation__menu__section'><Link to='/match-definition-contest'>Перевод-слово</Link></li>\r\n                    <li className='practice-navigation__menu__section'><Link to='/word-definition-contest'>Слово-перевод</Link></li>\r\n                </ul>\r\n            </nav>\r\n        </div>\r\n    )\r\n}","C:\\Users\\Ultra\\Desktop\\Practice\\wisdom\\src\\components\\practice\\practice-type\\practice-type.js",["131","132","133","134"],"import React, { useEffect, useState } from 'react'\r\nimport { getDictionary } from \"../../../services/dictionary\";\r\nimport { Route, BrowserRouter as Router, Link, Switch } from 'react-router-dom'\r\n\r\nexport const PracticeTypeMaraphon = () => {\r\n    const [words, setWords] = useState([])\r\n\r\n    const getAllWords = async () => {\r\n        try {\r\n            const allWordsFromServer = await getDictionary()\r\n            setWords(allWordsFromServer)\r\n        } catch (error) {\r\n            console.error(error)\r\n        }\r\n    }\r\n\r\n    useEffect(() => {\r\n        getAllWords()\r\n    }, [])\r\n\r\n\r\n    const [timer, setTimer] = useState(30);\r\n    const [running, setRunning] = useState(true);\r\n\r\n    useEffect(() => {\r\n        if (running) {\r\n            const id = window.setInterval(() => {\r\n                setTimer(seconds => seconds > 1 ? seconds - 1 : '');\r\n            }, 1000)\r\n            return () => window.clearInterval(id);\r\n        }\r\n    }, [running]);\r\n\r\n    const numberGenerator = Math.floor(Math.random() * 140);\r\n    const [num, setNum] = useState(numberGenerator);\r\n\r\n    const handleGenerator = () => {\r\n        setNum(numberGenerator);\r\n        setAnswer('');\r\n    }\r\n\r\n    // есть ли другой способ?\r\n    const showRandomWord = (num) => {\r\n        for (let word of words) {\r\n            if (word.id === num) {\r\n                return word.word;\r\n            } else {\r\n                continue\r\n            }\r\n        }\r\n    }\r\n\r\n    const [answer, setAnswer] = useState('');\r\n\r\n    const handleChanger = (evt) => {\r\n        const { value } = evt.target;\r\n        setAnswer(value);\r\n    }\r\n\r\n    const handleKeyPress = (evt) => {\r\n        if (evt.keyCode === 13 && answer === showRandomWord(num)) {\r\n            setNum(numberGenerator);\r\n            setAnswer('');\r\n            setPoints(prevState => prevState + 5);\r\n        }\r\n    }\r\n\r\n    const [points, setPoints] = useState(0);\r\n\r\n    const newGame = () => {\r\n        handleGenerator();\r\n        setTimer(30);\r\n        setPoints(0);\r\n        setAnswer('');\r\n    }\r\n\r\n    return (\r\n        <div className='container'>\r\n            <div className='type-character-contest'>\r\n                <div className='type-character-contest__wrapper'>\r\n                    <Link className='type-character-contest__exit' to='/practice'><span className='type-character-contest__exit__link'>Выйти из задания</span></Link>\r\n                    <span className={timer === '' ? 'type-character-contest__timer type-character-contest__timer_off' : 'type-character-contest__timer'}>\r\n                        {timer}\r\n                    </span>\r\n                    <div className='type-character-contest__current-word'>\r\n                        <span>\r\n                            {timer === '' ? 'Время вышло!' : showRandomWord(num)}\r\n                        </span>\r\n                    </div>\r\n                    <div className='type-character-contest__points'>{`Очков заработано: ${points}`}</div>\r\n                    <label className='type-character-contest__answer-label'>\r\n                        <input\r\n                            className={answer === showRandomWord(num) ? 'type-character-contest__answer-input type-character-contest__answer-input_correct' : 'type-character-contest__answer-input'}\r\n                            type='text'\r\n                            onChange={handleChanger}\r\n                            value={answer}\r\n                            hidden={timer === ''}\r\n                            onKeyDown={handleKeyPress}\r\n                        />\r\n                    </label>\r\n                    {answer === showRandomWord(num) && timer !== '' ?\r\n                        <button\r\n                            disabled={!answer}\r\n                            onClick={handleGenerator}\r\n                            className='type-character-contest__answer-is-correct'>\r\n                            Верно!\r\n                    </button> :\r\n                        timer === '' ? null :\r\n                            <div className='type-character-contest__answers-awaiting'>\r\n                                {!answer ? 'Впишите слово!' : 'Неверно!'}\r\n                            </div>}\r\n                    {timer === '' ? <button\r\n                        type='button'\r\n                        className='type-character-contest__re-try'\r\n                        onClick={newGame}\r\n                    >Хочу ещё раз!</button> : null}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}","C:\\Users\\Ultra\\Desktop\\Practice\\wisdom\\src\\components\\practice\\practice-matching\\practice-matching-definition\\practice-matching-definition.js",["135","136","137"],"import React, { useEffect, useState } from 'react'\r\nimport { getDictionary } from '../../../../services/dictionary';\r\nimport { Route, BrowserRouter as Router, Link, Switch } from 'react-router-dom'\r\n\r\nexport const PracticeDefinitionMatching = () => {\r\n    // можно ли прокинуть из одного файла во все?\r\n    const [words, setWords] = useState([])\r\n    const getAllWords = async () => {\r\n        try {\r\n            const allWordsFromServer = await getDictionary()\r\n            setWords(allWordsFromServer)\r\n        } catch (error) {\r\n            console.error(error)\r\n        }\r\n    }\r\n    useEffect(() => {\r\n        getAllWords()\r\n    }, [])\r\n\r\n    const arrayShuffle = (arr) => {\r\n        let newPos, temp;\r\n        for (let i = arr.length - 1; i > 0; i--) {\r\n            newPos = Math.floor(Math.random() * (i + 1))\r\n            temp = arr[i];\r\n            arr[i] = arr[newPos];\r\n            arr[newPos] = temp;\r\n        }\r\n        return arr;\r\n    }\r\n\r\n    const [practice, setPractice] = useState([])\r\n    // console.log('practice', practice) => массив при первой загрузке пуст, аналогично в словаре\r\n    // из-за этого к этой опции привязана логики отрисовки, нужно поправить\r\n\r\n    const renderQuiz = () => {\r\n        setStatus(false)\r\n        setPractice(arrayShuffle(words).filter((_, i) => i < 4));\r\n    }\r\n\r\n    //to start\r\n    const startTask = () => {\r\n        setPractice(arrayShuffle(words).filter((_, i) => i < 4));\r\n    }\r\n\r\n    let num = Math.floor(Math.random() * 4);\r\n\r\n    const [status, setStatus] = useState(false)\r\n    const validation = (evt) => {\r\n  